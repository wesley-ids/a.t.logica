programa
{
	
	funcao inicio()
	{
		inteiro D, pr, de
		cadeia p
		escreva("QUAL PRODUTO VOCÊ QUER COMPRAR? ")
		leia (p)
		escreva("QUANTO ELE CUSTA? ")
		leia (pr)
		D = pr / 10
		de = pr - D
		escreva("O PREÇO FINAL DESSE PRODUTO COM DESCONTO É: ", de ,"R$")
		
	}
}
